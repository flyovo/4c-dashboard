y: 'sidebarTextTheme', value })\n  }\n\n  private themeChange(value: string) {\n    SettingsModule.ChangeSetting({ key: 'theme', value })\n  }\n\n  private async logout() {\n    await UserStoreModule.Logout()\n    this.$router.push(`/login?redirect=${this.$route.fullPath}`)\n    let loading: any\n    loading = Loading.service({\n      fullscreen: true,\n      text: 'Loading',\n      spinner: 'el-icon-loading',\n      background: 'rgba(0, 0, 0, 0.7)'\n    })\n    setTimeout(() => {\n      window.location.reload()\n    }, 100)\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.drawer-container {\n  .btn-logout {\n    text-align: right;\n    border-bottom: 1px solid #e86e3d;\n    .el-button {\n      margin-bottom: 10px;\n      color: gray;\n      border-color: transparent;\n      background-color: transparent;\n    }\n  }\n  padding: 24px;\n  font-size: 14px;\n  line-height: 1.5;\n  word-wrap: break-word;\n\n  .drawer-title {\n    // border-bottom: 1px solid #e86e3d;\n    margin-bottom: 12px;\n    color: rgba(0, 0, 0, 0.85);\n    font-size: 14px;\n    line-height: 22px;\n  }\n\n  .drawer-item {\n    color: rgba(0, 0, 0, 0.65);\n    font-size: 14px;\n    padding: 12px 0;\n  }\n\n  .drawer-switch {\n    float: right;\n  }\n}\n.bottom {\n  text-align: right;\n  line-height: 12px;\n}\n.clearfix:before,\n.clearfix:after {\n  display: table;\n  content: '';\n}\n\n.clearfix:after {\n  clear: both;\n}\n</style>\n"]}]}