;AAAA;AAAA,WAsHS,2BAAkB,IAAlB,EAAmC;AACxC,WAAK,mBAAL,CAAyB,IAAzB;AACD;AAxHH;AAAA;AAAA,EAAuB,gCAAvB,CAAA;;AAKE,uBAAA,CADC,8BACD,CAAA,E,kBAAA,E,kBAAA,EAOC,IAPD;AAUA,uBAAA,CADC,8BACD,CAAA,E,kBAAA,E,iBAAA,EAKC,IALD;AAQA,uBAAA,CADC,8BACD,CAAA,E,kBAAA,E,kBAAA,EAOC,IAPD;AAUA,uBAAA,CADC,8BACD,CAAA,E,kBAAA,E,iBAAA,EAGC,IAHD;AAMA,uBAAA,CADC,8BACD,CAAA,E,kBAAA,E,0BAAA,EAIC,IAJD;AAOA,uBAAA,CADC,8BACD,CAAA,E,kBAAA,E,yBAAA,EAQC,IARD;AAWA,uBAAA,CADC,8BACD,CAAA,E,kBAAA,E,uBAAA,EAIC,IAJD;AAOA,uBAAA,CADC,8BACD,CAAA,E,kBAAA,E,sBAAA,EAEC,IAFD;AAKA,uBAAA,CADC,8BACD,CAAA,E,kBAAA,E,qBAAA,EAOC,IAPD;AAUA,uBAAA,CADC,kCAAO;AAAE,EAAA,QAAQ,EAAE;AAAZ,CAAP,CACD,CAAA,E,kBAAA,E,SAAA,EAGC,IAHD;AAMA,uBAAA,CADC,kCAAO;AAAE,EAAA,QAAQ,EAAE;AAAZ,CAAP,CACD,CAAA,E,kBAAA,E,gBAAA,EAEC,IAFD;AAKA,uBAAA,CADC,kCAAO;AAAE,EAAA,QAAQ,EAAE;AAAZ,CAAP,CACD,CAAA,E,kBAAA,E,SAAA,EAGC,IAHD;AAMA,uBAAA,CADC,kCAAO;AAAE,EAAA,QAAQ,EAAE;AAAZ,CAAP,CACD,CAAA,E,kBAAA,E,eAAA,EAEC,IAFD;AAKA,uBAAA,CADC,kCAAO;AAAE,EAAA,QAAQ,EAAE;AAAZ,CAAP,CACD,CAAA,E,kBAAA,E,gBAAA,EAGC,IAHD;AAMA,uBAAA,CADC,kCAAO;AAAE,EAAA,QAAQ,EAAE;AAAZ,CAAP,CACD,CAAA,E,kBAAA,E,aAAA,EAGC,IAHD;AAMA,uBAAA,CADC,kCAAO;AAAE,EAAA,QAAQ,EAAE;AAAZ,CAAP,CACD,CAAA,E,kBAAA,E,mBAAA,EAEC,IAFD;AAKA,uBAAA,CADC,kCAAO;AAAE,EAAA,QAAQ,EAAE;AAAZ,CAAP,CACD,CAAA,E,kBAAA,E,mBAAA,EAEC,IAFD;AAtHI,QAAQ,GAAA,uBAAA,CADb,kCAAO;AAAE,EAAA,OAAO,EAAE,IAAX;AAAiB,EAAA,KAAK,EAAL,cAAjB;AAAwB,EAAA,IAAI,EAAE,UAA9B;AAA0C,EAAA,UAAU,EAAE;AAAtD,CAAP,CACa,CAAA,EAAR,QAAQ,CAAR;AA2HC,IAAM,cAAc,GAAG,qCAAU,QAAV,CAAvB","sourcesContent":["import store from '@/store'\n\nimport { TagViewItem, TagsViewState } from './type'\nimport { VuexModule, Module, Mutation, Action, getModule } from 'vuex-module-decorators'\n\n@Module({ dynamic: true, store, name: 'tagsView', namespaced: true })\nclass TagsView extends VuexModule implements TagsViewState {\n  public visitedViews: TagViewItem[] = []\n  public cachedViews: (string | undefined)[] = []\n\n  @Mutation\n  private ADD_VISITED_VIEW(view: TagViewItem) {\n    if (this.visitedViews.some(v => v.path === view.path)) return\n    this.visitedViews.push(\n      Object.assign({}, view, {\n        title: view.meta.title || 'no-name'\n      })\n    )\n  }\n\n  @Mutation\n  private ADD_CACHED_VIEW(view: TagViewItem) {\n    if (this.cachedViews.includes(view.name)) return\n    if (!view.meta.noCache) {\n      this.cachedViews.push(view.name)\n    }\n  }\n\n  @Mutation\n  private DEL_VISITED_VIEW(view: TagViewItem) {\n    for (const [i, v] of this.visitedViews.entries()) {\n      if (v.path === view.path) {\n        this.visitedViews.splice(i, 1)\n        break\n      }\n    }\n  }\n\n  @Mutation\n  private DEL_CACHED_VIEW(view: TagViewItem) {\n    const index = this.cachedViews.indexOf(view.name)\n    index > -1 && this.cachedViews.splice(index, 1)\n  }\n\n  @Mutation\n  private DEL_OTHERS_VISITED_VIEWS(view: TagViewItem) {\n    this.visitedViews = this.visitedViews.filter(v => {\n      return v.meta.affix || v.path === view.path\n    })\n  }\n\n  @Mutation\n  private DEL_OTHERS_CACHED_VIEWS(view: TagViewItem) {\n    const index = this.cachedViews.indexOf(view.name)\n    if (index > -1) {\n      this.cachedViews = this.cachedViews.slice(index, index + 1)\n    } else {\n      // if index = -1, there is no cached tags\n      this.cachedViews = []\n    }\n  }\n\n  @Mutation\n  private DEL_ALL_VISITED_VIEWS() {\n    // keep affix tags\n    const affixTags = this.visitedViews.filter(tag => tag.meta.affix)\n    this.visitedViews = affixTags\n  }\n\n  @Mutation\n  private DEL_ALL_CACHED_VIEWS() {\n    this.cachedViews = []\n  }\n\n  @Mutation\n  private UPDATE_VISITED_VIEW(view: TagViewItem) {\n    for (let v of this.visitedViews) {\n      if (v.path === view.path) {\n        v = Object.assign(v, view)\n        break\n      }\n    }\n  }\n\n  @Action({ rawError: true })\n  public addView(view: TagViewItem) {\n    this.ADD_VISITED_VIEW(view)\n    this.ADD_CACHED_VIEW(view)\n  }\n\n  @Action({ rawError: true })\n  public addVisitedView(view: TagViewItem) {\n    this.ADD_VISITED_VIEW(view)\n  }\n\n  @Action({ rawError: true })\n  public delView(view: TagViewItem) {\n    this.DEL_VISITED_VIEW(view)\n    this.DEL_CACHED_VIEW(view)\n  }\n\n  @Action({ rawError: true })\n  public delCachedView(view: TagViewItem) {\n    this.DEL_CACHED_VIEW(view)\n  }\n\n  @Action({ rawError: true })\n  public delOthersViews(view: TagViewItem) {\n    this.DEL_OTHERS_VISITED_VIEWS(view)\n    this.DEL_OTHERS_CACHED_VIEWS(view)\n  }\n\n  @Action({ rawError: true })\n  public delAllViews() {\n    this.DEL_ALL_VISITED_VIEWS()\n    this.DEL_ALL_CACHED_VIEWS()\n  }\n\n  @Action({ rawError: true })\n  public delAllCachedViews() {\n    this.DEL_ALL_CACHED_VIEWS()\n  }\n\n  @Action({ rawError: true })\n  public updateVisitedView(view: TagViewItem) {\n    this.UPDATE_VISITED_VIEW(view)\n  }\n}\n\nexport const TagsViewModule = getModule(TagsView)\n"],"sourceRoot":""}]}